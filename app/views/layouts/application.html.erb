<!DOCTYPE html>
<html>
  <head>
    <title>ContactsAppClient</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div>
      <a href="/client/signup">Sign Up</a>
    </div>
    <div>
      <a href="/client/login">Log In</a>
    </div>
    <div>
      <a href="/client/logout" data-method="delete">Log Out</a>
    </div>
    <%= yield %>
  </body>
</html>
